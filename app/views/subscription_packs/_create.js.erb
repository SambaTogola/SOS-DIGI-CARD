$("ul.errors").html("");
<% if @subscription_pack.errors.any? %>
  <% @subscription_pack.errors.full_messages.each do |message| %>
 
    $("ul.errors").append($('<div class="c-alert c-alert--danger" > ').html("<%= message.html_safe %> "));
  <% end %>
<% else %>
  $(".subscription-packs").html("<%= escape_javascript(render 'index') %>");
  $("#subscription-pack-modal").modal("hide");
 
<% end %>