<% @members.each do |member| %>
  <tr>
    <td><%= member.created_at.strftime("%d/%m/%Y") %></td>
    <td><%= member.organization.name %></td>
    <td><%= member.service.name %></td>
    <td><%= member.user %></td>
    <td><%= member.position %></td>
    <td><%= member.status %></td>
    
    <td>
                    <div class="action-buttons">
                    




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), service_delete_path(member), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
  </tr>
<% end %>