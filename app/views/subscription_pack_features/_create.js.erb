$("ul.errors").html("");
<% if @subscription_pack_feature.errors.any? %>
  <% @subscription_pack_feature.errors.full_messages.each do |message| %>
 
    $("ul.errors").append($('<div class="c-alert c-alert--danger" > ').html("<%= message.html_safe %> "));
  <% end %>
<% else %>
  $(".subscription-pack-features").html("<%= escape_javascript(render 'index') %>");
  $("#subscription-pack-feature-modal").modal("hide");
 
<% end %>